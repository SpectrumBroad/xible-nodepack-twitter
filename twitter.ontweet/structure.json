{
  "name": "twitter.ontweet",
  "type": "event",
  "description": "Triggered whenever a tweet comes in on the given keywords or usernames to follow.",
  "inputs": {
    "twitter": {
  		"type": "twitter"
  	}
  },
  "outputs": {
    "trigger": {
  		"type": "trigger"
  	},
    "tweet": {
  		"type": "object"
  	},
    "text": {
  		"type": "string"
  	},
    "username": {
  		"type": "string"
  	}
  }
}
